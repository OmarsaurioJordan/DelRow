            <string>var dt = delta_time * 0.000001;

antispam = max(0, antispam - dt);

reloj_notifi = max(0, reloj_notifi - dt);

reloj_objeto -= dt;
if reloj_objeto &lt;= 0 {
    reloj_objeto = 12;
}

reloj_bandera -= dt;
if reloj_bandera &lt;= 0 {
    reloj_bandera = random_range(2, 4);
    bandera = irandom(m_map_total - 1);
}

